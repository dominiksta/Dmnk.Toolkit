{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "./src",
          "files": [
            "Dmnk.Blazor.Cookies/bin/Release/net10.0/Dmnk.Blazor.Cookies.dll",
            "Dmnk.Blazor.Dialogs/bin/Release/net10.0/Dmnk.Blazor.Dialogs.dll",
            "Dmnk.Blazor.Dialogs.Fluent/bin/Release/net10.0/Dmnk.Blazor.Dialogs.Fluent.dll",
            "Dmnk.Blazor.Focus/bin/Release/net10.0/Dmnk.Blazor.Focus.dll",
            "Dmnk.Blazor.Mvvm/bin/Release/net10.0/Dmnk.Blazor.Mvvm.dll",
            "Dmnk.Blazor.OverlayScrollbar/bin/Release/net10.0/Dmnk.Blazor.OverlayScrollbar.dll",
            "Dmnk.Blazor.Storage/bin/Release/net10.0/Dmnk.Blazor.Storage.dll",
            "Dmnk.Blazor.Suspense/bin/Release/net10.0/Dmnk.Blazor.Suspense.dll",
            "Dmnk.Icons.Blazor/bin/Release/net10.0/Dmnk.Icons.Blazor.dll",
            "Dmnk.Icons.Blazor.Fluent/bin/Release/net10.0/Dmnk.Icons.Blazor.Fluent.dll",
            "Dmnk.Icons.Core/bin/Release/netstandard2.0/Dmnk.Icons.Core.dll",
          ],
          "exclude": [
            "**/obj/**",
            "**/test/**"
          ]
        }
      ],
      "dest": "docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "*.{md,yml}"
        ],
        "src": "docs"
      },
      {
        "files": [
          "index.md",
          "toc.yml"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "**/README.md"
        ],
        "src": "./src"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "docs"
      }
    ],
    "output": "docs/_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Dmnt.Toolkit.Docs",
      "_appTitle": "Dmnt.Toolkit.Docs",
      "_enableSearch": true,
      "_appHomePage": "README.md",
      "pdf": false
    }
  }
}